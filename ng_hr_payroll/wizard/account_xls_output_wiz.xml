<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<data>

		<record id="account_xls_output_view" model="ir.ui.view">
			<field name="name">Excel Output</field>
			<field name="model">account.xls.output.wiz</field>
			<field name="arch" type="xml">
				<form string="Excel Output">
					<separator colspan="4" string="Please save file as .xls excel format" />
					<field name="name" />
					<field name="xls_output" filename="name" />
					<button special="cancel" string="Close" icon="gtk-cancel" />
				</form>
			</field>
		</record>
	</data>
</odoo>
